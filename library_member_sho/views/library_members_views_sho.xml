<odoo>

    <record id="view_form_book_inherit" model="ir.ui.view">
        <field name="name">Book Form Inherit</field>
        <field name="model">library.book.sho</field>
        <field name="arch" type="xml">

            <field name="isbn" position="after">
                <field name="is_available" />
            </field>

        </field>
    </record>

    <record id="view_tree_book_inherit" model="ir.ui.view">
        <field name="name">Book List Inherit</field>
        <field name="model">library.book.sho</field>
        <field name="arch" type="xml">

                <field name="publisher_id" position="after" >
                    <field name="is_available" />
                </field>

        </field>
    </record>

</odoo>