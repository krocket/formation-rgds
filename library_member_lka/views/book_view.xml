<?xml version="1.0"?>
<odoo>
  <record id="view_form_book_extend" model="ir.ui.view">
    <field name="name">Book: add Is Available? field</field>
    <field name="model">library.book.lka</field>
    <field name="inherit_id" ref="library_app_lka.view_form_book"/>
    <field name="arch" type="xml">
      <field name="isbn" position="after">
        <field name="is_available" />
      </field>
    </field>
  </record>

  <record id="view_tree_book_extend" model="ir.ui.view">
    <field name="name">Book list</field>
    <field name="model">library.book.lka</field>
    <field name="inherit_id" ref="library_app_lka.view_tree_book"/>
    <field name="arch" type="xml">
      <field name="name" position="after">
        <field name="is_available" />
      </field>
    </field>
  </record>
</odoo>
